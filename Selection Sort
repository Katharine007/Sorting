def sort(x):
    n=len(x)
    for i in range(n-1):
        min_index=i
        for j in range(i+1,n):
            if x[j]<x[min_index]:
                min_index=j
                
        min_val=x.pop(min_index)
        x[i]=min_val
                
x = [64, 34, 25, 12, 22, 11, 90]
print(x)
sort(x)
print("Sorted array using Sort:", x)


